#: floppy.cpp:119
#, kde-format
msgid "F&ile system:"
msgstr "&Súborový systém:"

#: floppy.cpp:126 floppy.cpp:136
#, kde-format
msgctxt "Linux"
msgid ""
"KFloppy supports three file formats under Linux: MS-DOS, Ext2, and Minix"
msgstr "Pod Linuxom podporuje KFloppy tieto tri formáty: MS-DOS, Ext2, a Minix"

#: floppy.cpp:128
#, kde-format
msgctxt "BSD"
msgid "KFloppy supports three file formats under BSD: MS-DOS, UFS, and Ext2"
msgstr ""
"KFloppy podporuje pod systémom BSD tieto tri formáty: MS-DOS, UFS, a Ext2"

#: floppy.cpp:138 floppy.cpp:163 floppy.cpp:558 floppy.cpp:660
#, kde-format
msgid "DOS"
msgstr "DOS"

#: floppy.cpp:140
#, kde-format
msgctxt "Linux"
msgid "Program mkdosfs found."
msgstr "Program mkdosfs nájdený."

#: floppy.cpp:142
#, kde-format
msgctxt "Linux"
msgid ""
"Program mkdosfs <b>not found</b>. MSDOS formatting <b>not available</b>."
msgstr ""
"Program mkdosfs <b>nenájdený</b>. MSDOS formátovanie <b>je nedostupné</b>."

#: floppy.cpp:146 floppy.cpp:179 floppy.cpp:569
#, kde-format
msgid "ext2"
msgstr "ext2"

#: floppy.cpp:148 floppy.cpp:181
#, kde-format
msgid "Program mke2fs found."
msgstr "Program mke2fs nájdený."

#: floppy.cpp:150 floppy.cpp:183
#, kde-format
msgid "Program mke2fs <b>not found</b>. Ext2 formatting <b>not available</b>"
msgstr ""
"Program mke2fs <b>nenájdený</b>. Ext2 formátovanie <b>je nedostupné</b>"

#: floppy.cpp:154 floppy.cpp:588
#, kde-format
msgid "Minix"
msgstr "Minix"

#: floppy.cpp:156
#, kde-format
msgctxt "Linux"
msgid "Program mkfs.minix found."
