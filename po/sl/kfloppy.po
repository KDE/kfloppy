#: floppy.cpp:119
#, kde-format
msgid "F&ile system:"
msgstr "Da&totečni sistem:"

#: floppy.cpp:126 floppy.cpp:136
#, kde-format
msgctxt "Linux"
msgid ""
"KFloppy supports three file formats under Linux: MS-DOS, Ext2, and Minix"
msgstr "KFloppy v Linuxu podpira tri datotečne sisteme: MS-DOS, Ext2 in Minix"

#: floppy.cpp:128
#, kde-format
msgctxt "BSD"
msgid "KFloppy supports three file formats under BSD: MS-DOS, UFS, and Ext2"
msgstr "KFloppy v BSD podpira tri datotečne sisteme: MS-DOS, UFS in Ext2"

#: floppy.cpp:138 floppy.cpp:163 floppy.cpp:558 floppy.cpp:660
#, kde-format
msgid "DOS"
msgstr "DOS"

